    "lint-staged": "^10.4.0",
    "lodash": "^4.17.20",
    "lodash.camelcase": "^4.3.0",
    "prettier-eslint": "^11.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "pretty-quick": "^3.0.2",
    "prompt": "^1.0.0",
