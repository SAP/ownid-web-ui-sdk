    "rollup": "^2.28.2",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "rollup-plugin-typescript2": "^0.27.2",
    "rollup-plugin-uglify": "^6.0.4",
    "semantic-release": "^17.1.2",
    "shelljs": "^0.8.4",
    "ts-jest": "^26.4.0",
    "ts-node": "~9.0.0",
    "typedoc": "^0.19.2",
    "typescript": "4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick"
