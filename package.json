{
  "name": "ownid-web-ui-sdk",
  "version": "1.0.0-alpha-0.0.1",
  "description": "",
  "keywords": [],
  "main": "dist/sdk.umd.js",
  "module": "dist/sdk.es5.js",
  "typings": "dist/types/sdk.d.ts",
  "files": [
    "dist"
  ],
  "author": "SAP",
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "prebuild": "rimraf dist && rimraf out",
    "build2": "tsc && rollup -c rollup.config.ts && typedoc --out docs --target es6 --theme minimal --mode file src",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "yarn lint && yarn test -- --no-cache",
    "test:ci": "jest --runInBand",
    "lint": "eslint . --ext .ts --fix",
    "verify": "yarn test && yarn lint && yarn build",
    "format": "prettier-eslint --write $PWD/'src/**/*.{[jt]s, html}' && eslint . --ext .ts --fix"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@types/jest": "^26.0.8",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "codelyzer": "^6.0.0",
    "colors": "^1.4.0",
    "commitizen": "^4.1.2",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.2.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-unicorn": "^21.0.0",
    "husky": "^4.2.5",
    "jest": "^26.2.2",
    "jest-config": "^26.2.2",
    "jest-resolver-tsconfig-paths": "^0.12.1",
    "jest-trx-results-processor": "^2.0.3",
    "lint-staged": "^10.2.11",
    "lodash.camelcase": "^4.3.0",
    "prettier-eslint": "^11.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "pretty-quick": "^2.0.1",
    "prompt": "^1.0.0",
    "replace-in-file": "^6.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.23.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "rollup-plugin-typescript2": "^0.27.1",
    "rollup-plugin-uglify": "^6.0.4",
    "semantic-release": "^17.1.1",
    "shelljs": "^0.8.4",
    "travis-deploy-once": "^5.0.11",
    "ts-jest": "^26.1.4",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.3",
    "typedoc": "^0.17.8",
    "typescript": "3.9.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick"
    }
  },
  "dependencies": {
    "qrcode-generator": "^1.4.4",
    "whatwg-fetch": "^3.3.1"
  },
